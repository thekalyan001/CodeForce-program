agar beech me hoga number to wo hoga beautyful matrix -3,3,per



code---->>>>>

#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int a[5][5];int p,q;
	for(int i=0;i<5;i++)
	{
		for(int j=0;j<5;j++)
		{
			cin>>a[i][j];
			if(a[i][j]==1){p=i;q=j;}
		}
	}
	p=abs(p-2);
	q=abs(q-2);
	cout<<p+q;
return 0;
}
	
